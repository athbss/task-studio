{
   "name": "task-studio",
   "version": "0.1.1-beta.14",
   "description": "Web-based UI for Taskmaster task management system",
   "keywords": [
      "taskmaster",
      "task",
      "management",
      "ui",
      "web",
      "visualization"
   ],
   "homepage": "https://github.com/udecode/task-studio",
   "bugs": {
      "url": "https://github.com/udecode/task-studio/issues"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/udecode/task-studio.git"
   },
   "license": "MIT",
   "bin": {
      "task-studio": "./bin/index.js"
   },
   "files": [
      "bin/**/*",
      ".next/standalone/**/*",
      ".next/static/**/*",
      "public/**/*",
      "scripts/ws.js",
      "package.json"
   ],
   "scripts": {
      "dev": "concurrently \"npm run dev:next\" \"npm run dev:ws\"",
      "dev:next": "next dev --turbopack -p 5565",
      "dev:ws": "node scripts/ws.js",
      "build": "next build && node scripts/prepare-package.js",
      "start": "concurrently \"npm run start:next\" \"npm run start:ws\"",
      "start:next": "next start -p 5565",
      "start:ws": "node scripts/ws.js",
      "lint": "next lint",
      "typecheck": "tsc --noEmit",
      "format": "prettier --write .",
      "release:beta": "npm version prerelease --preid=beta --no-git-tag-version && npm publish --tag beta",
      "release:patch": "pnpm run build && npm version patch --no-git-tag-version && npm publish",
      "release:minor": "pnpm run build && npm version minor --no-git-tag-version && npm publish",
      "release:major": "pnpm run build && npm version major --no-git-tag-version && npm publish"
   },
   "publishConfig": {
      "access": "public"
   },
   "dependencies": {
      "@hookform/resolvers": "^4.1.2",
      "@kayron013/lexorank": "^2.0.0",
      "@radix-ui/react-alert-dialog": "^1.1.6",
      "@radix-ui/react-avatar": "^1.1.3",
      "@radix-ui/react-checkbox": "^1.1.4",
      "@radix-ui/react-collapsible": "^1.1.3",
      "@radix-ui/react-context-menu": "^2.2.10",
      "@radix-ui/react-dialog": "^1.1.6",
      "@radix-ui/react-dropdown-menu": "^2.1.6",
      "@radix-ui/react-label": "^2.1.2",
      "@radix-ui/react-popover": "^1.1.6",
      "@radix-ui/react-progress": "^1.1.2",
      "@radix-ui/react-radio-group": "^1.3.7",
      "@radix-ui/react-scroll-area": "^1.2.9",
      "@radix-ui/react-select": "^2.1.6",
      "@radix-ui/react-separator": "^1.1.2",
      "@radix-ui/react-slot": "^1.1.2",
      "@radix-ui/react-switch": "^1.1.3",
      "@radix-ui/react-tabs": "^1.1.3",
      "@radix-ui/react-toggle": "^1.1.2",
      "@radix-ui/react-tooltip": "^1.1.8",
      "@remixicon/react": "^4.6.0",
      "@tanstack/react-query": "^5.81.5",
      "chokidar": "^4.0.3",
      "class-variance-authority": "^0.7.1",
      "commander": "^14.0.0",
      "concurrently": "^9.2.0",
      "clsx": "^2.1.1",
      "cmdk": "1.0.0",
      "date-fns": "^4.1.0",
      "dotenv": "^17.0.0",
      "lucide-react": "^0.476.0",
      "motion": "^12.4.10",
      "next": "15.2.4",
      "open": "^10.1.0",
      "next-themes": "^0.4.4",
      "nuqs": "^2.4.3",
      "react": "^19.0.0",
      "react-day-picker": "8.10.1",
      "react-dnd": "^16.0.1",
      "react-dnd-html5-backend": "^16.0.1",
      "react-dom": "^19.0.0",
      "react-hook-form": "^7.54.2",
      "react-icons": "^5.5.0",
      "recharts": "^2.15.1",
      "sonner": "^2.0.1",
      "tailwind-merge": "^3.0.2",
      "tailwindcss-animate": "^1.0.7",
      "usehooks-ts": "^3.1.1",
      "uuid": "^11.1.0",
      "ws": "^8.18.3",
      "zod": "^3.24.2",
      "zustand": "^5.0.3"
   },
   "devDependencies": {
      "@eslint/eslintrc": "^3",
      "@tailwindcss/postcss": "^4",
      "@types/node": "^20",
      "@types/react": "^19",
      "@types/react-dom": "^19",
      "@types/ws": "^8.18.1",
      "eslint": "^9",
      "eslint-config-next": "15.2.4",
      "eslint-config-prettier": "^10.0.2",
      "eslint-plugin-prettier": "^5.2.3",
      "prettier": "^3.5.2",
      "tailwindcss": "^4",
      "typescript": "^5"
   }
}
